          "field": "__count",
          "sort": {"field": ["Origin"], "order": ["descending"]},
          "as": ["__count_start", "__count_end"],
          "offset": "zero"
        },
        {
          "type": "filter",
          "expr": "isValid(datum[\"bin_maxbins_10_Displacement\"]) && isFinite(+datum[\"bin_maxbins_10_Displacement\"])"
